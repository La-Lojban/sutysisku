    "mkdirp": "^1.0.4",
    "natural": "^5.0.3",
    "nodemw": "~0.14.0",
    "object-hash": "^2.2.0",
    "ospath": "~1.2.2",
    "path-extra": "~4.3.0",
    "peggy": "^1.2.0",
    "rakutenma": "^1.0.0",
